{
  "command": {
    "publish": {
      "allowBranch": [
        "develop",
        "master",
        "feature/"
      ],
      "conventionalCommits": true,
      "loglevel": "verbose",
      "message": "chore(release): publish",
      "yes": true
    },
    "run": {
      "stream": false
    },
    "version": {
      "allowBranch": [
        "develop",
        "master",
        "feature/"
      ],
      "conventionalCommits": true,
      "ignoreChanges": [],
      "loglevel": "verbose",
      "message": "chore(release): publish",
      "yes": true
    }
  },
  "loglevel": "error",
  "npmClientArgs": [
    "--no-progress"
  ],
  "packages": [
    "packages/*"
  ],
  "version": "independent"
}
